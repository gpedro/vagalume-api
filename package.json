    "babel-preset-env": "^1.5.2",
    "babel-preset-es2017": "^6.24.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "jest": "^20.0.4",
    "querystring": "^0.2.0"
  }
